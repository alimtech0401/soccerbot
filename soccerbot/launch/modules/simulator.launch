<?xml version="1.0"?>
<launch>
  <env name="WEBOTS_HOME" value="/usr/local/webots" />
  <include file="$(find webots_ros)/launch/webots.launch">
    <arg name="world" value="$(find soccer_webots)/worlds/flat_world.wbt"/>
    <arg name="mode" value="realtime"/>
    <arg name="no_gui" value="false"/>
  </include>

</launch>
